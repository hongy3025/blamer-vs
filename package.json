{
    "name": "svn-blame",
    "displayName": "SVN Blame",
    "description": " SVN blame code line-by-line",
    "version": "0.5.2",
    "publisher": "yinghong",
    "repository": {
        "type": "git",
        "url": "https://github.com/hongy3025/blamer-vs.git"
    },
    "bugs": {
        "url": "https://github.com/hongy3025/blamer-vs/issues"
    },
    "homepage": "https://github.com/hongy3025/blamer-vs",
    "icon": "img/blamer.png",
    "license": "MIT",
    "engines": {
        "vscode": "^1.87.0"
    },
    "categories": [
        "SCM Providers",
        "Other"
    ],
    "activationEvents": [],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "svn-blame.Show",
                "title": "SVN Blame Show"
            },
            {
                "command": "svn-blame.Clear",
                "title": "SVN Blame Clear"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "SVN Blame configuration",
            "properties": {
                "svn-blame.enablePips": {
                    "type": "boolean",
                    "default": true,
                    "description": "Enable/Disable visual 'pip' decoration, that sits alongside the line number."
                }
            }
        }
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "pnpm run lint",
        "test": "vscode-test"
    },
    "devDependencies": {
        "@types/mocha": "^10.0.6",
        "@types/node": "^20.11.30",
        "@types/vscode": "^1.87.0",
        "@vscode/test-cli": "^0.0.8",
        "@vscode/test-electron": "^2.3.9",
        "eslint": "^8.57.0",
        "typescript": "^5.4.3"
    }
}